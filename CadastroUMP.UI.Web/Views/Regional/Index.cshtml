@model IEnumerable<CadastroUMP.Dominio.Regional>

@{
    ViewBag.Title = "Index";
}

<h2>Lista de Regionais</h2>

<p>
    @Html.ActionLink("Cadastrar", "Cadastrar")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NomeRegional)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomeRegional)
            </td>
            <td>
                @Html.ActionLink("Editar", "Editar", new { id = item.RegionalId }) |
            @Html.ActionLink("Detalhes", "Detalhes", new { id = item.RegionalId }) |
            @Html.ActionLink("Excluir", "Excluir", new { id = item.RegionalId })
            </td>
        </tr>
    }

</table>
